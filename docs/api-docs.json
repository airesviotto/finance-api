                                "properties": {
                                    "description": {
                                        "type": "string",
                                        "example": "Grocery shopping"
                                    },
                                    "amount": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 120.5
                                    },
                                    "currency": {
                                        "type": "string",
                                        "example": "USD"
                                    },
                                    "type": {
                                        "type": "string",
